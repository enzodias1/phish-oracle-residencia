	document.addEventListener('DOMContentLoaded', (event) => {
		const languageSwitcherMappings = {
			'ko': 'https://liftoff.cn/ko/',
			'es': 'https://liftoff.cn/es/',
			'pt_BR': 'https://liftoff.cn/pt-br/',
			'ja': 'https://liftoff.cn/ja/',
			'zh-Hans': 'https://liftoff.cn/zh-hans/',
			'en': 'https://liftoff.cn/',
		};

		for (const [dataCode, url] of Object.entries(languageSwitcherMappings)) {
			let languageSwitcher = document.querySelector(`[data-code="${dataCode}"]`);
			
			if(languageSwitcher) {
				languageSwitcher.addEventListener('click', (event) => {
				event.preventDefault();
				window.location.href = url;
			});
		}
		}
	});
