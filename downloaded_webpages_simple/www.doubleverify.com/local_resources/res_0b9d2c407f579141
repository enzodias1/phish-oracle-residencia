var animController = new ScrollMagic.Controller();
$(document).ready(function(){
	$('.jp-contact-form .us-states').hide();
	$( "form.contact-form .has-chosen").not('.select-country').find('select option:first-child').attr("disabled","disabled");
	$('form.contact-form').trigger("chosen:updated");

	$('.flag-dropdown span.default').on('click', function(){
        $('.flag-dropdown-content').toggle();
    });
	
	$('form').attr('autocomplete', 'off');
	
	if(getCookie('dv_user_visited') == ""){
		$('.home-special-overlay').addClass('active');
		setCookie('dv_user_visited', true, 30);
	}else{
		$('html').removeClass('overlay-active');
	}

	if (window.location.href.indexOf("#contact-form") > -1) {
    	$('html, body').animate({
	        //scrollTop: $('#contact').offset().top - 57
	        scrollTop: $('#contact').offset().top - $('#masthead').outerHeight()
	    }, 200);
    }

    if (window.location.href.indexOf("#signup") > -1) {
    	$('html, body').animate({
	        //scrollTop: $('#contact').offset().top - 57
	        scrollTop: $('.ctv-form-block').offset().top - $('#masthead').outerHeight()
	    }, 200);
    }
		
	$('.video-container_video').attr('playsinline', 'true');
	$('.fadeInToTop,.fadeInFromleft,.fadeInFromright').each(function(){   

		var commonScene = new ScrollMagic.Scene({
			triggerElement: this,
			triggerHook: .85,
		})
		.setClassToggle(this,'inView')
		.addTo(animController);
	});	

	var locationDetailScene = new ScrollMagic.Scene({
		triggerElement: '.location-new',
		triggerHook: .85,
		duration: '700%', 
	})
	.setClassToggle('.location-new','inView')
	.addTo(animController);
	
	enableLocationAnimation();
	$(window).on('resize', function() {
		enableLocationAnimation();
	});
	
	
	$('.openDetail-js').on('click',function(e){ 
		e.preventDefault(); 
		$('html').addClass('overlay-active');
		$('.home-sticky-overlay').addClass('active');
	});

	
	$('.overlay-close').on('click',function(){ 
		$('html').removeClass('overlay-active');
		$('.home-sticky-overlay, .home-special-overlay').removeClass('active');
	});
	moveScroller();


	$('.privacy-filter-trigger').on('click', function(){
		$('.privacy-filter-trigger').removeClass('active');
		$(this).addClass('active');
		var selectedPolicy = $(this).attr('data-policy');
		$('.privacy-tabs').removeClass('active');
		$('.'+selectedPolicy).addClass('active');
	});

	$('.tab-filter-solution').on('click', function(){
		$('.privacy-filter-trigger').removeClass('active');
		$('button[data-policy=tab-product-policy]').addClass('active');
		$('.privacy-tabs').removeClass('active');
		$('.tab-product-policy').addClass('active');
		return false;
	});

	$('.tab-filter-website-and-marketing').on('click', function(){
		$('.privacy-filter-trigger').removeClass('active');
		$('button[data-policy=tab-privacy-policy]').addClass('active');
		$('.privacy-tabs').removeClass('active');
		$('.tab-privacy-policy').addClass('active');
		return false;
	});

	$('.download-form-anchor').on('click', function(e){
		e.preventDefault();
		$('html,body').animate({scrollTop:$('.ctv-form-block form').offset().top - 80}, 800);
		$('.ctv-form-block form input[name=your-name]').focus();
	});

	$('.hero-scrollto-btn').on('click', function(){
		event.preventDefault();
    	$('html,body').animate({scrollTop:$($(this).attr('href')).offset().top}, 800);
	});

	$('a[target="_blank"').each(function(){
        if($(this).attr('rel', 'noreferrer') || $(this).attr('rel', 'noopener') || $(this).attr('rel', 'noopener noreferrer')) {
            $(this).removeAttr("rel");
            $(this).attr('rel', 'noopener noreferrer');
        }
    });	

    $('.jetpack-forms-wrapper .grunion-field-wrap input').on('focus', function(){
    	$(this).parents('label.placeholder-wrapper').addClass('active');
    	$(this).parents('label.placeholder-wrapper').removeClass('not-focused');
    });
    $('.jetpack-forms-wrapper .grunion-field-wrap input').on('blur', function(){
    	if($(this).val() != ''){
    		$(this).parents('label.placeholder-wrapper').addClass('not-focused');
    	}else{
	    	$(this).parents('label.placeholder-wrapper').removeClass('active not-focused');
	    }
    });

    $('.jetpack-forms-wrapper .grunion-field-wrap textarea').on('focus', function(){
    	$(this).parents('label.text-area-label').addClass('active');
    	$(this).parents('label.text-area-label').removeClass('not-focused');
    });
    $('.jetpack-forms-wrapper .grunion-field-wrap textarea').on('blur', function(){
    	if($(this).val() != ''){
    		$(this).parents('label.text-area-label').addClass('not-focused');
    	}else{
	    	$(this).parents('label.text-area-label').removeClass('active not-focused');
	    }
    	
    });
	
	
	//Timeline slider
	var timelineYearSlider = $('.tlist1');
	timelineYearSlider.owlCarousel({     
		loop: false,
		items: 7,
		margin: 0,
		nav: false,
		dots: false,
		center: false,
		smartSpeed: 800,
		mouseDrag: true,
		touchDrag: false,
		rewindNav: true,
		rewindSpeed: 0,
		responsive:{
			0:{
				items:3
			},
			600:{
				items:5
			},
			1000:{
                items:7,
                stagePadding: 50
			}
        }
    });
    
	$('.tlist1 .owl-item').click(function(){ 
		$('.tlist1').addClass('inActive');
        var clickedIndex = $(this).index();
        timelineYearSlider.trigger('to.owl.carousel', clickedIndex);
        timelineInfoSlider.trigger('to.owl.carousel', clickedIndex);
        slideControl = clickedIndex+1;
        

		setTimeout(function(){ 
			$('.tlist1').removeClass('inActive');
		}, 1000);

	});


	var timelineInfoSlider = $('.tinfo1');
	timelineInfoSlider.owlCarousel({
		loop: false,
		items: 1,
		margin: 0,
		nav: false,
		dots: false,
		center: false,
		smartSpeed: 800,
		mouseDrag: false,
		touchDrag: false,
		onInitialized: checkIndex,
	});  
	var infoTotalSlide,slideControl = 1;
	function checkIndex(e) {
		infoTotalSlide = e.item.count;
	}

	$('.customOwlNav .owl-prev').on('click', function (e) {
		timelineInfoSlider.find('.owl-prev').trigger('click');           
	});
	$('.customOwlNav .owl-next').on('click', function () {
		timelineInfoSlider.find('.owl-next').trigger('click');       
	}); 
	timelineInfoSlider.on('click', '.owl-prev', function (e) {
		slideControl--;
		if(slideControl >= 1 ) {                
			timelineYearSlider.trigger('prev.owl.carousel');
		} else {
			slideControl = 1;
		}            
	});
	timelineInfoSlider.on('click', '.owl-next', function () {
		slideControl++;
		if(slideControl <= infoTotalSlide ) {                
			timelineYearSlider.trigger('next.owl.carousel');
		} else {
			slideControl = infoTotalSlide;
		}            
	});    

	var timelineYearSlider2 = $('.tlist2');
	timelineYearSlider2.owlCarousel({     
		loop: false,
		items: 7,
		margin: 0,
		nav: true,
		dots: false,
		center: true,
		smartSpeed: 800,
		mouseDrag: true,
		touchDrag: false,
		rewindNav: true,
		rewindSpeed: 0,
		responsive:{
			0:{
				items:3
			},
			600:{
				items:5
			},
			1000:{
				items:7
			}
        },
        onTranslate: function(){
            $('.owl-item').removeClass('next-item');
        },
        onInitialized: function(){
            $('.tlist2 .owl-item.center').next().addClass('next-item');
        }
    });
    
    
	$('.tlist2 .owl-item').click(function(){ 
		$('.tlist2').addClass('inActive');
		var clickedIndex = $(this).index();
        timelineYearSlider2.trigger('to.owl.carousel', clickedIndex);
        timelineInfoSlider2.trigger('to.owl.carousel', clickedIndex);

		setTimeout(function(){ 
			$('.tlist2').removeClass('inActive');
		}, 1000);

	});

	timelineYearSlider2.on('click', '.owl-prev', function () {
		timelineInfoSlider2.find('.owl-prev').trigger('click');        
        timelineYearSlider2.find('.owl-item.center').prev().addClass('next-item');
        timelineYearSlider2.find('.owl-item.center').next().addClass('next-item');
	});
	timelineYearSlider2.on('click', '.owl-next', function () {
		timelineInfoSlider2.find('.owl-next').trigger('click');   
        timelineYearSlider2.find('.owl-item.center').next().addClass('next-item');
        timelineYearSlider2.find('.owl-item.center').prev().addClass('next-item');
    }); 
    
	var timelineInfoSlider2 = $('.tinfo2');
	timelineInfoSlider2.owlCarousel({
		loop: false,
		items: 1,
		margin: 0,
		nav: false,
		dots: false,
		center: false,
		smartSpeed: 800,
		mouseDrag: false,
		touchDrag: false,
		onInitialized: checkIndex,
	});   
	//Timeline slider 
	
