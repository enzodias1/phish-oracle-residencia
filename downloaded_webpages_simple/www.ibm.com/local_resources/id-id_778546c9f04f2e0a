       bpid: document.getElementById("ibm-widget-form-bpId").value, // Optional. pass bpid to pre-fill business partner name and ID.
       design: {
         column: 3,
         theme: document.getElementById("ibm-widget-form-theme").value , // dark
         singleStep: true,
       },
       triggerManually: false,
       onUrxFormSubmit: function (params, callback) { // Optional
         // params: Form data & targetUrl
         // Example promise function
         setTimeout(function (params){
           // do whatever you want to do with the data
           callback(); // trigger callback so that URX redirect the user to the target page
           // If you do not call this callback withing 10 seconds, urx will automatically redirect the user to the target url
         }, 2000);
       },
       formLoaded: function() {
       // This function will gets triggered when form is loaded.
       },
       onUrxFormSubmitSuccess: function (params) {
           // This function will gets triggered when form submitted successfully.
           console.log('success callback data', params)
           window.location.href = params.targetUrl;
           // jQuery('#urx-form2').hide();
           // jQuery('#urx-form2').html('<h1 class="ibm-h1">Thank you! </h1><p>This message is being shown by the custom code written in the callback function "onUrxFormSubmitSuccess"</p>').show();
       },
       userActionEvent: function (action, payload){
       // userActionEvent callback function
       // action: action name, example: REG_FORM_SUBMITTED, FORM_SUBMIT_FAIL, ACTIVE_SESSION_IDENTIFIED,
       // payload: user info or other data. example: { email: 'xxx@ccc.com', firstName: 'John', lastName: 'Doe', countr: 'IN'}

          console.info('userActionEvent', action, payload);
       },
     }
  ]);
 </script>
