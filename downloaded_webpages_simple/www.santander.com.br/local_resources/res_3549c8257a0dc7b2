                                        <script>
                                            var dataLayer = window.dataLayer || []
                                            var pagePath = document.location.pathname;
                                            if (pagePath === "/") {
                                                var customPath = "/home";
                                            } else {
                                                var customPath = "/home-empresas";
                                            }
                                            window.dataLayer.push({
                                                event: 'pageviewGA',
                                                page: customPath
                                            });
                                            document.body.setAttribute("onresize", "displayWidthDetect()");

                                            let menuFixedPf = document.querySelector('.pf');
                                            let menuFixedPj = document.querySelector('.pj');


                                            if (window.location.href.includes('empresas')) {

