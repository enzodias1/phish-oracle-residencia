      var t = doc.createElement('script');
      t.nonce = '0YIi9/yF9NvVvrjAXwTukw==';
      if (typeof c.s === 'function') {
        if (c.id) t.id = c.id;
        t.textContent = cts('('+c.s+').apply(window);');
