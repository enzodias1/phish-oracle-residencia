    <script type="text/javascript">
  (function(win) {
    var config = win._sf_async_config || {};
    config.uid = config.uid || 1;
    config.sections = config.sections || '';
    config.authors = config.authors || '';
    config.sessionLength = config.sessionLength || 1440;
    config.domain = config.domain || "chartbeat.com";
    config.pingServer = config.pingServer || "pong.chartbeat.net";
    config.moduleAttribute = "data-module";

    

    

    

    
    win._sf_async_config = config;

    if(typeof(ga) !== 'undefined') {
      config.idSync = config.idSync || {};

      ga(function(tracker) {
        config.idSync.ga = tracker.get('clientId');
      });
    }
  }(window));
</script>


    
    
