                                        <div class="floating-container">
                                            <a href="https://abrasuaconta.santander.com.br/?ic=homepf:flutuante:abra-sua-conta" target="_blank"
                                                class="floating-button">Abra sua conta</a>
                                            <a href="https://www.santander.com.br/banco/conta-corrente/select/?ic=homepf:flutuante:conta-select" target="_blank"
                                                class="floating-button">Select</a>
                                        </div>
                                        <script>
                                            const floatingElementButtons = document.getElementsByClassName('floating-button');
                                            const floatingContainerElement = document.querySelector('.floating-container')

                                            const floatingButtonColorPrimary = 'Branco';
                                            const floatingButtonColorSecondary = 'Vermelho';
                                            const floatingContainerColor = 'Branco';

                                            const CONTAINER_STYLES = {
                                                'Preto': 'floating-container-black',
                                                'Branco': 'floating-container-white',
                                                'Vermelho': 'floating-container-red',
                                            }

                                            const BUTTON_STYLES = {
                                                'Vermelho': 'primary-button',
                                                'Branco': 'outline-button'
                                            }

                                            const formatText = (text) => {
                                                return text
                                                    .toLowerCase()
                                                    .normalize("NFD")
                                                    .replace(/[\u0300-\u036f]/g, "")
                                                    .replace(/[^a-z0-9\s]/g, "")
                                                    .trim()
                                                    .replace(/\s+/g, "-");
                                            };

                                            const applyContainerStyle = (buttons, element, color) => {
                                                if (!Array.from(buttons).length) {
                                                    
                                                    return;
                                                }

                                                if (!element) {
                                                    return;
                                                }


                                                element.classList.add(CONTAINER_STYLES[color]);

                                            };

                                            const applyButtonStyle = (button, color) => {
                                                if (!button) return;

                                                button.classList.add(BUTTON_STYLES[color]);
                                            };

                                            const removeButtonIfEmpty = (button) => {
                                                if (!button) return;

                                                const buttonText = button.textContent.trim();

                                                if (!buttonText) {
                                                    button.remove();

                                                    const remainingButtons = document.getElementsByClassName('floating-button');

                                                    const hasButtonWithText = Array.from(remainingButtons).some(btn => btn.textContent.trim());
                                                    
                                                    if (!hasButtonWithText) {
                                                        const container = document.querySelector('.floating-container');
                                                        if (container) {
                                                            container.style.display = 'none';
                                                        }
                                                    }
                                                }
                                            };

                                            removeButtonIfEmpty(floatingElementButtons[0], floatingContainerElement);
                                            removeButtonIfEmpty(floatingElementButtons[1], floatingContainerElement);

                                            applyContainerStyle(floatingElementButtons, floatingContainerElement, floatingContainerColor);
                                            applyButtonStyle(floatingElementButtons[0], floatingButtonColorPrimary);
                                            applyButtonStyle(floatingElementButtons[1], floatingButtonColorSecondary);

                                            document.querySelectorAll('.floating-button').forEach(button => {
                                                button.addEventListener('click', () => {
                                                    const buttonText = button.textContent.trim();

                                                    if (!buttonText) {
                                                        return;
                                                    }
                                                    const formattedText = formatText(buttonText)

                                                    if (!formattedText || formattedText.length === 0) {
                                                        return;
                                                    }

                                                    const eventLabel = 'botao:flutuante:' + formattedText;

                                                    setEventGa({
                                                        category: 'portal:home',
                                                        label: eventLabel
                                                    });
                                                });
                                            });
                                        </script>

