         <script type="text/javascript">

           function getParameterByName(name, url) {
              if (!url) url = window.location.href;
              name = name.replace(/[\[\]]/g, '\\$&');
              var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
                  results = regex.exec(url);
              if (!results) return null;
              if (!results[2]) return '';
              return decodeURIComponent(results[2].replace(/\+/g, ' '));
           }
           
           searchableText = getParameterByName("SearchableText");
           tagBusca = getParameterByName("Subject%3Alist");
           if (!tagBusca) {
              tagBusca = getParameterByName("Subject:list");
           }

           document.addEventListener("DOMContentLoaded", function(event) {
             let urlBase = document.getElementById("url_base").value;
             if (!urlBase.endsWith("/")) {
               urlBase += "/";
             }

             let busca = new govBrBusca.Busca({
               target: document.getElementById("buscaContainer"),
               props: {
                 urlBase: urlBase,
                 urlPortal: document.getElementById("site_url").value,
                 portalNome: document.getElementById("portal_nome").value,
                 categoriasFiltro: JSON.parse(document.getElementById("categorias_filtro").value),
                 orgaosFiltro: JSON.parse(document.getElementById("orgaos_filtro").value),
                 orgaoId: document.getElementById("orgao_id").value,
                 textoBusca: searchableText,
                 tagBusca: tagBusca,
                 gruposTipos: {
                   "Midia": [
                     {
                       "descricao": "Arquivo",
                       "tipos": ["File"]
                     },
                     {
                       "descricao": "Áudio",
                       "tipos": ["Audio", "MPEG Audio File", "OGG Audio File"]
                     },
                     {
                       "descricao": "Conteúdo Externo",
                       "tipos": ["External Content"]
                     },
                     {
                       "descricao": "Imagem",
                       "tipos": ["Image"]
                     },
                     {
                       "descricao": "Multimídia",
                       "tipos": ["sc.embedder"]
                     },
                   ]
                 },
                 categorias: [
                   {
                     "descricao": "Serviços",
                     "tipos": document.getElementById("aba_servicos").value.split(","),
                     "idAba": "servicos"
                   },
                   {
                     "descricao": "Serviços Estaduais",
                     "tipos": document.getElementById("aba_servicos_estaduais").value.split(","),
                     "idAba": "servicos_estaduais"
                   },
                   {
                     "descricao": "Notícias",
                     "tipos": document.getElementById("aba_noticias").value.split(","),
                     "idAba": "noticias"
                   },
                   {
                     "descricao": "Sites",
                     "tipos": document.getElementById("aba_sites").value.split(","),
                     "idAba": "sites"
                   },
                   {
                     "descricao": "Apps",
                     "tipos": document.getElementById("aba_apps").value.split(","),
                     "idAba": "apps"
                   },
                   {
                     "descricao": "Todos",
                     "tipos": document.getElementById("aba_todos").value.split(","),
                     "idAba": "todos"
                   }
                 ],
                 ordenacao: '-relevancia',
                 tamanhoPagina: 30
               }
             });
           });

