                                        <!-- Script Swiper -->
                                        <script
                                            src="/sites/WPC_Banco/assets/js/lp_pj_file/swiper-bundle.min.js"></script>
                                        <script>
                                            (function () {
                                                function initializeSwiper() {
                                                    var interval = 500;
                                                    var maxTime = 20000;
                                                    var totalTime = 0;

                                                    var checkInterval = setInterval(function () {
                                                        totalTime += interval;

                                                        var jsLoaded = (typeof Swiper !== 'undefined');
                                                        var elementExists = document.querySelector('.cards-slider') !== null;

                                                        if (jsLoaded && elementExists) {
                                                            initSwiper();
                                                            clearInterval(checkInterval);
                                                        } else if (totalTime >= maxTime) {
                                                            console.error('Failed to initialize Swiper: JS not loaded or element not found within 20000ms.');
                                                            clearInterval(checkInterval);
                                                        }
                                                    }, interval);
                                                }

                                                function initSwiper() {
                                                    let cardsSwiper = new Swiper('.cards-slider', {
                                                        freeMode: true,
                                                        touchEventsTarget: 'container',
                                                        loop: false,
                                                        grabCursor: true,
                                                        slidesPerView: "auto",
                                                        navigation: {
                                                            nextEl: ".cards-slider__swiper-button-next",
                                                            prevEl: ".cards-slider__swiper-button-prev",
                                                        }
                                                    });
                                                    cardsSwiper.update();
                                                }

                                                initializeSwiper();
                                            })();
                                        </script>

                                        <script>
                                             function handleElementOnScroll(selector, footerSelector, offset = 0) {
                                                const element = document.querySelector(selector);
                                                const footer = document.querySelector(footerSelector);
                                                if (!element || !footer) {
                                                    return;
                                                }

                                                element.style.transition = 'opacity 0.4s, transform 0.4s';

                                                function toggle() {
                                                    const footerTop = footer.getBoundingClientRect().top;
                                                    const windowHeight = window.innerHeight;
                                                    if (footerTop - offset < windowHeight) {
                                                        element.style.opacity = '0';
                                                        element.style.transform = 'translateY(100%)';
                                                        element.style.pointerEvents = 'none';
                                                    } else {
                                                        element.style.opacity = '';
                                                        element.style.transform = '';
                                                        element.style.pointerEvents = '';
                                                    }
                                                }

                                                window.addEventListener('scroll', toggle);
                                                window.addEventListener('resize', toggle);
                                                document.addEventListener('DOMContentLoaded', toggle);
                                                toggle();
                                            }

                                            handleElementOnScroll('.floating-container', '.copy-fot');
                                        </script>
                                        <script>
                                            // HOTFIX BOTÃO REDIRECT F1RST 18/06/2025
                                            (() => {
                                            const POLL_MS = 200;
                                            const MAX_ATTEMPTS = 50;
                                            let tries = 0;
                                            
                                            // heurÃ­stica simples: detecta PJ ou PF ignorando acentos e
                                            // tolerando caracteres quebrados (ex.: Ã, ï¿½)
                                            const detectTipo = raw => {
                                                const lower = raw.toLowerCase();
                                            
                                                if (lower.includes('empresa')) return 'PJ';      // Para sua Empresa
                                                if (lower.includes('voc'))     return 'PF';      // Para vocÃª / Para vocÃÂª / Para vocï¿½ ...
                                            
                                                return null; // nÃ£o reconhecido
                                            };
                                            
                                            const timer = setInterval(() => {
                                                if (++tries > MAX_ATTEMPTS) return clearInterval(timer);
                                            
                                                const host = document.querySelector(
                                                'body > nav.headerfixed > wpc-mfe-home-header-element'
                                                );
                                                if (!host) return;
                                            
                                                const shadow = host.shadowRoot;
                                                if (!shadow) return;
                                            
                                                const titleEl = shadow.querySelector(
                                                '#redirect > span.menuMbb_title_different.dss-h1.dss-color-blue-light2'
                                                );
                                                const redirectEl = shadow.querySelector('#redirect');
                                                if (!titleEl || !redirectEl) return;
                                            
                                                const rawText = titleEl.textContent.trim();
                                                const tipo = detectTipo(rawText); // 'PF', 'PJ' ou null
                                            
                                                const urlMap = {
                                                PF: 'https://www.santander.com.br/',
                                                PJ: 'https://www.santander.com.br/empresas'
                                                };
                                            
                                                if (tipo) {
                                                redirectEl.addEventListener(
                                                    'click',
                                                    () => {
                                                    window.location.href = urlMap[tipo];
                                                    },
                                                    { once: true }
                                                );
                                                clearInterval(timer); // encerra o polling apÃ³s configurar
                                                }
                                            }, POLL_MS);
                                            })();
                                        </script>
