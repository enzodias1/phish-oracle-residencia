		loop: false,
		items: 1,
		margin: 0,
		nav: true,
		dots: false,
		center: true,
		smartSpeed: 800,
		mouseDrag: false,
		touchDrag: false,
		rewindNav: true,
		rewindSpeed: 0,
		responsive:{
			0:{
				stagePadding:30,
				margin: 10
			},
			768:{
				stagePadding:100,
				margin: 25
			},
			992:{
				stagePadding:240,
				margin: 30
			},
			1200:{
				stagePadding:285,
				margin: 35
			}
		},
		onInitialized: totalInitialized,
		onChanged: totalInitialized,
	});	


		
	

	
	$('.wpcf7-submit').removeAttr('disabled');
	$("input[name='your-accepted-terms'").prop('required',true);
	
	// Blog filter customization
	
	
	$('[name=category]').on('click', function(){
		var filter = $(this).parents('form');
		var cat = [];
		$.each($("#filter-blog-post input[name='category']:checked"), function(){
			cat.push($(this).val());
		});

		var tags = [];
		$.each($("#filter-blog-post-tag input[name='post_tag']:checked"), function(){
			tags.push($(this).val());
		});
		
		$.ajax({
			url:filter.attr('action'),
			data:'category=' + cat + '&tag=' + tags.toString() + '&action=filter_post_ajax', // form data
			type:'POST', // POST
			success:function(data){
				$('.tag-blog').html(data);
				//$('.tag-blog .btn').attr('data-category', cat);
			}
		});
		
		
		$.ajax({
			url:filter.attr('action'),
			data:'category=' + cat + '&tag=' + 'event,webinar' + '&action=filter_post_ajax', // form data
			type:'POST', // POST
			success:function(data){
				$('.tag-event').html(data);
				$('.tag-event .btn').attr('data-category', cat);
			}
		});
		
	});


	$('[name=post_tag]').on('click', function(){
		var filter = $(this).parents('form');

		var cat = [];
		$.each($("#filter-blog-post input[name='category']:checked"), function(){
			cat.push($(this).val());
		});

		var tags = [];
		$.each($("#filter-blog-post-tag input[name='post_tag']:checked"), function(){
			tags.push($(this).val());
		});

		$.ajax({
			url:filter.attr('action'),
			data:'category=' + cat + '&tag=' + tags.toString() + '&action=filter_post_ajax', // form data
			type:'POST', // POST
			success:function(data){
				$('.tag-blog').html(data);
				//$('.tag-blog .btn').attr('data-category', cat);
			}
		});
	});

	$('[name=spotlight_post_category]').on('change', function(){
		var filter = $(this).parents('form');
		var cat = $('[name=spotlight_post_category]').val();
		var tags = $('[name=spotlight_post_tag]').val();
		var page_id = $('[name=page_id]').val();
		$('.spotlight-loadmore-cta').hide();
		
		$.ajax({
			url:filter.attr('action'),
			data:'category=' + cat + '&tag=' + tags + '&action=filter_post_ajax&page_name=spotlight&page_id='+page_id, // form data
			type:'POST', // POST
			success:function(data){
				$('.latest-post-wrap').html(data);
				if($('[name=spotlight_page_post_count]').val() > 3){
					$('.spotlight-loadmore-cta').show();
				}
				$('.spotlight-loadmore-cta').attr('data-category', cat);
			}
		});		
	});

	$('[name=spotlight_post_tag]').on('change', function(){
		var filter = $(this).parents('form');
		var cat = $('[name=spotlight_post_category]').val();
		var tags = $('[name=spotlight_post_tag]').val();
		var page_id = $('[name=page_id]').val();
		$('.spotlight-loadmore-cta').hide();
		
		$.ajax({
			url:filter.attr('action'),
			data:'category=' + cat + '&tag=' + tags + '&action=filter_post_ajax&page_name=spotlight&page_id='+page_id, // form data
			type:'POST', // POST
			success:function(data){
				$('.latest-post-wrap').html(data);
				if($('[name=spotlight_page_post_count]').val() > 3){
					$('.spotlight-loadmore-cta').show();
				}
				$('.spotlight-loadmore-cta').attr('data-tag', tags);
			}
		});		
	});

	var filter = $('#filter-blog-post');
	var cat = [];
	$.each($("#filter-blog-post input[name='category']:checked"), function(){
		cat.push($(this).val());
	});

	var tags = [];
	$.each($("#filter-blog-post-tag input[name='post_tag']:checked"), function(){
		tags.push($(this).val());
	});

	if(cat.length != 0){
		$.ajax({
			url:filter.attr('action'),
			data:'category=' + cat + '&tag=' + tags.toString() + '&action=filter_post_ajax', // form data
			type:'POST', // POST
			success:function(data){
				$('.tag-blog').html(data);
				//$('.tag-blog .btn').attr('data-category', cat);
			}
		});
		
		
		$.ajax({
			url:filter.attr('action'),
			data:'category=' + cat + '&tag=' + 'event,webinar' + '&action=filter_post_ajax', // form data
			type:'POST', // POST
			success:function(data){
				$('.tag-event').html(data);
				$('.tag-event .btn').attr('data-category', cat);
			}
		}); 
		
	}
	
	if($('body').hasClass('single-post')){
		setCookie('wp_visited_post_cat', $('[name=wp_visited_post_cat]').val(), 30);
		setCookie('wp_visited_post_tag', $('[name=wp_visited_post_tag]').val(), 30);
	}
});

// vimeo video play btn code
function embedVideo(elm) {
	var videoId = $(elm).data('target');
	var toggle = $($(elm).data('toggle'));
	toggle.hide();

	var autoplay = 1;

	$(elm).show().html('<iframe src="https://player.vimeo.com/video/'+videoId+'" width="100%" height="100%" frameborder="0"  allow="autoplay; fullscreen" allowfullscreen="true" mozallowfullscreen="true" msallowfullscreen="true" oallowfullscreen="true" webkitallowfullscreen="true"></iframe>');
}

function setCookie(cname, cvalue, expireDate) {
	var d = new Date();
	d.setTime(d.getTime() + (expireDate * 24 * 60 * 60 * 1000));
	var expires = "expires="+d.toUTCString();
	document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function getCookie(cname) {
	var name = cname + "=";
	var decodedCookie = decodeURIComponent(document.cookie);

	var ca = decodedCookie.split(';');
	for(var i = 0; i <ca.length; i++) {
		var c = ca[i];
		while (c.charAt(0) == ' ') {
			c = c.substring(1);
		}
		if (c.indexOf(name) == 0) {
			return c.substring(name.length, c.length);
		}
	}
	return "";
}

function deleteCookie(cname) {
  document.cookie = cname +'=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
}

function enableLocationAnimation() {

	if($(window).width() < 768) {

		$('.office-locations .single-locations').each(function(){   

			var commonScene = new ScrollMagic.Scene({
				triggerElement: this,
				triggerHook: .85,
				duration: '700%', 
			})
			.setClassToggle(this,'inView')
			.addTo(animController);
		});		
	   
	} else {
		$('.sliding-js').each(function(){   

			var commonScene = new ScrollMagic.Scene({
				triggerElement: this,
				triggerHook: .95,
				duration: '700%', 
			})
			.setClassToggle(this,'inView')
			.addTo(animController);
		});	
		
	}	
}
function totalInitialized(event) {
	var totalOwls = event.item.count;
	var currentPos = event.item.index;
	$('#owlTotalSlide').text(totalOwls);
	$('#owlCurrentSlide').text(currentPos+1);
	
}
function moveScroller() {   
    $(window).on('scroll', function(e){
        if($('.stickyNode').length) {

            if ($(this).scrollTop() > ($('.stickyNode').offset().top) ){
                $('.stickypromo').addClass('isReady');

            } else {
                $('.stickypromo').removeClass('isReady');
            }               
        }
            
    });    
}


// Free mail service validator
function isValidEmailAddress(emailAddress) {
	return /^([^@]+@(?!gmail\.com)(?!live\.com)(?!mail\.com)(?!icloud\.com)(?!hushmail\.com)(?!gmx\.com)(?!outlook\.com)(?!aol\.com)(?!yahoo\.com)(?!hotmail\.com)(?!adelaidelift\.com)(?!adelaidemetrics\.com)(?!adloox\.com)(?!azerion\.com)(?!channelfactory\.com)(?!cheq\.ai)(?!comscore\.com)(?!confiant\.com)(?!grapeshot\.com)(?!gumgum\.com)(?!humansecurity\.com)(?!ispot\.tv)(?!integralads\.com)(?!kinetiq\.tv)(?!lumen\.com)(?!lumenad\.com)(?!meltwater\.com)(?!methodmi\.com)(?!mfilterit\.com)(?!moat\.com)(?!nanointeractive\.com)(?!nielsen\.com)(?!oracle\.com)(?!peer39\.com)(?!permutive\.com)(?!pixability\.com)(?!pixalate\.com)(?!playgroundxyz\.com)(?!sightly\.com)(?!themediatrust\.com)(?!trafficguard\.ai)(?!zefr\.com)(?!aminopay\.com)(?!admantx\.com)(?!adnimation\.com)(?!getpublica\.com)(?!phybbit\.com)(?!publica\.com)(?!spideraf\.com)(?!spotx\.com)(?!wearecontext\.com)(?!whiteops\.com)([\w-]+.)+[\w-]{2,4})?$/.test(emailAddress);
} 

// Special mail service validator
function isBlockedEmailAddress(emailAddress) {
	return /^([\w-.]+@(?!oracle)(?!integralads)([\w-]+.)+[\w-]{2,4})?$/.test(emailAddress);
}

// contact form cloner
(function ($) {	
	var formSource = '';
	if ((window.location.href).match(/\?./)) {
		var page_full_url = $(location).attr('href').split("?");
		formSource = page_full_url[0].split(".com");
	}else{
		var page_full_url = $(location).attr('href');
		formSource = page_full_url.split(".com");
	}

	var DV_Custom_Contextual = false;
	DV_Custom_Contextual = $('body').hasClass('postid-9801');
	
	var formType = "Contact Form";
	var event_form_success_msg = $('#event_form_success_msg').val();
	var event_form_consent_msg = $('#event_form_consent_msg').val();

	var d = new Date();
	var urlParams = new URLSearchParams(window.location.search);
	var utm_source = utm_campaign = utm_medium = utm_term = utm_content = "";
	var donwloadFile;
	var form_custom_value = '';
	var jp_contact_form = false;
	var region;
	var apac = Array('Australia', 'Bangladesh', 'China','Hong Kong','India','Indonesia', 'Japan', 'Malaysia', 'Myanmar (Burma)', 'Nepal', 'New Zealand','Pakistan','Philippines', 'Singapore','South Korea','Sri Lanka','Taiwan','Thailand','Vietnam' );
	var latam = Array('Argentina','Bolivia','Brazil','Chile','Colombia', 'Costa Rica', 'Domincan Republic', 'Ecuador','El Salvador', 'Guatemala', 'Honduras', 'Mexico', 'Nicaragua', 'Panama', 'Paraguay', 'Peru', 'Uruguay', 'Venezuela');
	var us_canada = Array('Canada','Puerto Rico');
	
	if(urlParams != ""){
		if(urlParams.has('utm_source')){
			utm_source = urlParams.get('utm_source');
		}else{
			utm_source = "";
		}
		setCookie("utm_source",utm_source,1);

		if(urlParams.has('utm_campaign')){
			utm_campaign = urlParams.get('utm_campaign');
		}else{
			utm_campaign = "";
		}
		setCookie("utm_campaign",utm_campaign,1);

		if(urlParams.has('utm_medium')){
			utm_medium = urlParams.get('utm_medium');
		}else{
			utm_medium = "";
		}
		setCookie("utm_medium",utm_medium,1);
		
		if(urlParams.has('utm_term')){
			utm_term = urlParams.get('utm_term');
		}else{
			utm_term = "";
		}
		setCookie("utm_term",utm_term,1);
		
		if(urlParams.has('utm_content')){
			utm_content = urlParams.get('utm_content');
		}else{
			utm_content = "";
		}
		setCookie("utm_content",utm_content,1);
	}else{
		utm_source = getCookie('utm_source');
		utm_campaign = getCookie('utm_campaign');
		utm_medium = getCookie('utm_medium');
		utm_term = getCookie('utm_term');
		utm_content = getCookie('utm_content');
	}

	if($('.ctv-form-block [name=donwload-ctv-file]').length > 0){
		donwloadFile = $('[name=donwload-ctv-file]').val();
	}
	
	$('a').on('click',function() {
		if($(this).attr('href') == '#download-pdf' || $(this).attr('href') == '#anyform' || $(this).hasClass('download-pdf-form')){
			formType = "Download PDF";	
			donwloadFile = $(this).parent().find('[name=donwload-pdf-file]').val();
			form_custom_value = $(this).parent().find('[name=form_custom_value]').val();

			if($(this).hasClass('modal-open-cta')){
				donwloadFile = $(this).next('.download-file').val();
			}
			
			//$('.download-pdf-modal .jetpack-forms-inner h3').html('Submit your information to complete the download');
			//$('.download-pdf-modal .jetpack-forms-inner .contact-submit button[type=submit]').html('Download PDF');
		}else if($(this).attr('href') == '#person-event'){
			formType = "In Person Event";	

			if($('body').hasClass('postid-639615') || $('body').hasClass('postid-703070')){
				formType = "Event";
			}
		}else if($(this).attr('href') == '#webinar-event' || $(this).attr('href') == '#webinar-event-jp' || $(this).attr('href') == '#webinar-contact-jp'){
			formType = "Webinar Event";	
		}else{
			formType = "Contact Form";
		}	

		if($(this).attr('href') == '#webinar-contact-jp'){
			jp_contact_form = true;
		}	

		if($('body').hasClass('postid-9894133')){
			console.log('資料ダウンロードはこちら。必要事項をご記入ください。');
			$('.webinar-event-jp-modal .jetpack-forms-inner h3').html("資料ダウンロードはこちら。必要事項をご記入ください。");
		}

	});

	
	if($('body').hasClass('page-id-11032')){
		$('.spotlight-ctv-form-block .wpcf7-form [name=your-accepted-terms]').siblings('.wpcf7-list-item-label').text('You consent to receive our Publicis Newsletter on an ongoing basis.');
	}

	var formId = "";
	var thisForm = "";
	var pageURL = "";
	var mkt_cookie = '';
	var mkt_formID = 1046;
	var payload = {};

	$('form.contact-form').on('click',function(){
		thisForm = $(this);
		formId = thisForm.find('input[name=contact-form-id]').val();
	});

	if(event_form_consent_msg != "" && typeof event_form_consent_msg != "undefined"){
		$('.wpcf7-form [name=your-accepted-terms]').siblings('.wpcf7-list-item-label').text(event_form_consent_msg);
	}
	
	$('form.contact-form input, form.contact-form textarea').on('blur', function(){
		payload['firstName'] = thisForm.find('input[name=g' + formId + '-firstname]').val();
		payload['lastName'] = thisForm.find('input[name=g' + formId + '-lastname]').val();
		payload['email'] = thisForm.find('input[name=g' + formId + '-emailaddress]').val();
		payload['title'] = thisForm.find('input[name=g' + formId + '-jobtitle]').val();
		payload['company'] = thisForm.find('input[name=g' + formId + '-companyname]').val();
		payload['Message__c'] = thisForm.find('textarea[name=g' + formId + '-message]').val();

		if(thisForm.parents('.jetpack-forms-wrapper').parent('div').attr('data-form_type')){
			formType = thisForm.parents('.jetpack-forms-wrapper').parent('div').attr('data-form_type');
		}else if(thisForm.parents('.jetpack-forms-inner').attr('data-form_type')){
			formType = thisForm.parents('.jetpack-forms-inner').attr('data-form_type');
		}

		if(formType != "Event"){
			if(utm_source != ""){
				payload['utmsource'] = utm_source;
			}

			if(utm_campaign != ""){
				payload['utmcampaign'] = utm_campaign;
			}

			if(utm_medium != ""){
				payload['utmmedium'] = utm_medium;
			}

			if(utm_term != ""){
				payload['utmterm'] = utm_term;
			}

			if(utm_content != ""){
				payload['utmcontent'] = utm_content;
			}
			
			payload['consentDate'] = d.getFullYear()+'-'+(d.getMonth()+1)+'-'+d.getDate()+' '+d.getHours()+'-'+d.getMinutes()+'-'+d.getSeconds();
		}
		payload['webFormType'] = formType;
		payload['formSource'] = pageURL = formSource[1];
	});

	function loadJobRole($this){
		jobRole1 = ($this.find('.job-role-select').attr('data-jobRole1')).split('#');
		jobRole2 = ($this.find('.job-role-select').attr('data-jobRole2')).split('#');
		jobRoleOptionName = $this.find('.job-role-select').attr('data-optionName');

		var jobRoleSelectOption = '<option value="'+jobRoleOptionName+'" >'+jobRoleOptionName+'</option>'; //disabled="disabled"
		for(var i = 0; i < jobRole1.length; i++){
			jobRoleSelectOption += '<option value="'+jobRole1[i]+'">'+jobRole1[i]+'</option>';
		}
		$this.find('.job-role-select select').html(jobRoleSelectOption).trigger('chosen:updated');
	}

	function loadAnnualSpend($this){
		annualSpend_1 = ($this.find('.annual-spend-select').attr('data-spend1')).split('#');
		annualSpend_2 = ($this.find('.annual-spend-select').attr('data-spend2')).split('#');
		annualSpend_full = annualSpend_1.concat(annualSpend_2);
		annualSpendOptionName = $this.find('.annual-spend-select').attr('data-optionName');

		var annualSpendSelectOption = '<option value="'+annualSpendOptionName+'" >'+annualSpendOptionName+'</option>';
		for(var i = 0; i < annualSpend_2.length; i++){
			annualSpendSelectOption += '<option value="'+annualSpend_2[i]+'">'+annualSpend_2[i]+'</option>';
		}
		$this.find('.annual-spend-select select').html(annualSpendSelectOption).trigger('chosen:updated');
	}


	$('form.contact-form').each(function(i,e){
		var formBase = $(this).parents('.jetpack-forms-inner');

		$("select").chosen().trigger("chosen:updated");

		formBase.find('.us-states').hide();
		if(formBase.find('.select-country select').val() == formBase.find('.select-country').attr('data-us')){
			formBase.find('.us-states').show();
		}
		
		var header = button = '';
		if(formBase.parent('.jetpack-forms-wrapper').parent('div').attr('data-form_header')){
			header = formBase.parent('.jetpack-forms-wrapper').parent('div').attr('data-form_header');
		}else if(formBase.parent('.jetpack-forms-wrapper').attr('data-form_header')){
			header = formBase.parent('.jetpack-forms-wrapper').attr('data-form_header');
		}
		if(header != ''){
			formBase.find('h3.h6').html(header);
		}

		if(formBase.parent('.jetpack-forms-wrapper').parent('div').attr('data-form_button')){
			button = formBase.parent('.jetpack-forms-wrapper').parent('div').attr('data-form_button');
		}else if(formBase.parent('.jetpack-forms-wrapper').attr('data-form_button')){
			button = formBase.parent('.jetpack-forms-wrapper').attr('data-form_button');
		}
		if(button != ''){
			formBase.find('.contact-form button[type="submit"]').text(button);
		}
		
		var companyType_1 = companyType_2 = [];
		var companyOptionName = '';
		if(formBase.find('.company-type-select').length){
			companyType_1 = (formBase.find('.company-type-select').attr('data-companyType1')).split('#');
			companyType_2 = (formBase.find('.company-type-select').attr('data-companyType2')).split('#');
			companyOptionName = formBase.find('.company-type-select').attr('data-optionName');
		}

		var jobRole1 = jobRole2 = [];
		var jobRoleOptionName = '';
		if(formBase.find('.job-role-select').length){
			loadJobRole(formBase);
		}

		var annualSpend_1 = annualSpend_2 = annualSpend_full = [];
		var annualSpendOptionName = '';
		if(formBase.find('.annual-spend-select').length){
			loadAnnualSpend(formBase);
		}	
		
	});

	
	$('form.contact-form select').on('change', function(){
		var thisForm = $(this).parents('.jetpack-forms-inner');
		var companyType = thisForm.find('select[name=g' + formId + '-companytype]').val();
		
		payload['Company_Type__c'] = companyType;

		if($(this).attr('name') == 'g' + formId + '-companytype'){
			var companyType_1 = (thisForm.find('.company-type-select').attr('data-companyType1')).split('#');
			var companyType_2 = (thisForm.find('.company-type-select').attr('data-companyType2')).split('#');
			var jobRole1 = (thisForm.find('.job-role-select').attr('data-jobRole1')).split('#');
			var jobRole2 = (thisForm.find('.job-role-select').attr('data-jobRole2')).split('#');
			var jobRoleOptionName = thisForm.find('.job-role-select').attr('data-optionName');
			var jobRoleSelectOption = '<option value="'+jobRoleOptionName+'" >'+jobRoleOptionName+'</option>'; //disabled="disabled"
			if(jQuery.inArray(companyType, companyType_1) >= 0){
				for(var i = 0; i < jobRole1.length; i++){
					jobRoleSelectOption += '<option value="'+jobRole1[i]+'">'+jobRole1[i]+'</option>';
				}
			}else if(jQuery.inArray(companyType, companyType_2) >= 0){
				for(var i = 0; i < jobRole2.length; i++){
					jobRoleSelectOption += '<option value="'+jobRole2[i]+'">'+jobRole2[i]+'</option>';
				}
			}

			$('.job-role-select select').html(jobRoleSelectOption).chosen().trigger('chosen:updated');
		}

		var co = thisForm.find('select[name=g' + formId + '-country]').val();
		var USA = thisForm.find('.select-country').attr('data-us');
		if(jQuery.inArray(USA, us_canada) <= 0){
			us_canada.push(USA);
		}
		var GER = thisForm.find('.select-country').attr('data-gr');
		var ci = st = '';

		if(co == GER){
			ci = thisForm.find('select[name=g' + formId + '-germancity]').val();
		}

		if(co == USA){
			st = thisForm.find('select[name=g' + formId + '-usstates]').val();
		}

		if(co == USA && st == "CA"){
			ci = thisForm.find('select[name=g' + formId + '-californiacity]').val();
		}

		if(co == USA && st == "TX"){
			ci = thisForm.find('select[name=g' + formId + '-texascity]').val();
		}

		if(co == USA && st == "FL"){
			ci = thisForm.find('select[name=g' + formId + '-floridacity]').val();
		}

		payload['city'] = ci;
		payload['state'] = st;
		payload['country'] = co;

		//var thisVal = $(formId + '.wpcf7-form select[name=your-country]').val();
		if(jQuery.inArray(co, latam) >= 0){
			region = 'LATAM';
		}else if(jQuery.inArray(co, apac) >= 0){
			region = 'APAC';
		}else if(jQuery.inArray(co, us_canada) >= 0){
			region = 'US and Canada';
		}else{
			region = 'EMEA';
		}

		payload['Region__c'] = region;

		var companyTypeSpend = (thisForm.find('.company-type-select').attr('data-company-spend')).split('#');

		if(jQuery.inArray(companyType, companyTypeSpend) >= 0){
			thisForm.find('.annual-spend-select').removeClass('hide-select');
		}else{
			thisForm.find('.annual-spend-select').addClass('hide-select');
		}

		if($(this).attr('name') != 'g' + formId + '-annualspend'){
			var annualSpend_1 = (thisForm.find('.annual-spend-select').attr('data-spend1')).split('#');
			var annualSpend_2 = (thisForm.find('.annual-spend-select').attr('data-spend2')).split('#');
			var annualSpend_full = annualSpend_1.concat(annualSpend_2);
			var annualSpendOptionName = thisForm.find('.annual-spend-select').attr('data-optionName');
			var companyType_1 = (thisForm.find('.company-type-select').attr('data-companyType1')).split('#');
			var companyType_2 = (thisForm.find('.company-type-select').attr('data-companyType2')).split('#');

			annualSpendSelectOption = '<option value="'+annualSpendOptionName+'" >'+annualSpendOptionName+'</option>';
			if((region == 'EMEA' || region == 'APAC') && jQuery.inArray(companyType, companyType_1) >= 0){
				for(var i = 0; i < annualSpend_1.length; i++){
					annualSpendSelectOption += '<option value="'+annualSpend_1[i]+'">'+annualSpend_1[i]+'</option>';
				}
			}else if((region == 'US and Canada' || region == 'LATAM') && jQuery.inArray(companyType, companyType_1) >= 0){
				for(var i = 0; i < annualSpend_2.length; i++){
					annualSpendSelectOption += '<option value="'+annualSpend_2[i]+'">'+annualSpend_2[i]+'</option>';
				}
			}else{
				for(var i = 0; i < annualSpend_full.length; i++){
					annualSpendSelectOption += '<option value="'+annualSpend_full[i]+'">'+annualSpend_full[i]+'</option>';
				}
			}
			$('.annual-spend-select select').html(annualSpendSelectOption).trigger('chosen:updated');
		}

		var jobLevel = thisForm.find('select[name=g' + formId + '-joblevel]').val();
		var jobRole = thisForm.find('select[name=g' + formId + '-jobrole]').val();
		var annualMediaSpend = thisForm.find('select[name=g' + formId + '-annualspend]').val();
		payload['jobLevel'] = jobLevel;
		payload['jobRole'] = jobRole;
		payload['annualMediaSpend'] = annualMediaSpend;
	});
	
	$('form.contact-form [type=checkbox]').on('click', function(){
		payload['consent'] = 'yes';
	});

	//NEW VIP JETPACK FORM CODES
	

	$('.select-country select').on('change', function(){
        var _this = $(this).val();
		var USA = $(this).parents('.jetpack-forms-inner').find('.select-country').attr('data-us');
		var GER = $(this).parents('.jetpack-forms-inner').find('.select-country').attr('data-gr');

        $('.jetpack-forms-wrapper .us-states, .jetpack-forms-wrapper .city-select').hide();
        if(_this == USA){
            $('.jetpack-forms-wrapper .us-states').show();
        }else if(_this == GER){
            $('.jetpack-forms-wrapper .german-city').show();
        }
    });

    $('.jetpack-forms-wrapper .us-states select').on('change', function(){
        var _this = $(this).val();
        $('.jetpack-forms-wrapper .city-select').hide();
        if(_this == 'CA'){
            $('.jetpack-forms-wrapper .california-city').show();
        }else if(_this == 'FL'){
            $('.jetpack-forms-wrapper .florida-city').show();
        }else if(_this == 'TX'){
            $('.jetpack-forms-wrapper .texas-city').show();
        }
    });

    $("form.contact-form button[type=submit]").on('click', function (e) {
        e.preventDefault();
        $('.placeholder-wrapper, .grunion-field-wrap').removeClass('error-field');
        $('.jetpack-form-ajax-status-wrapper').html('');

	    console.log(form_custom_value);
	    console.log(formType);
	    if($('body').hasClass('page-id-9885961')){
		console.log('paoa geche');
	}
	if($('body').hasClass('page-id-9885961') && formType == "Download PDF" && form_custom_value == ""){
		form_custom_value = 'Global';
	}

        var formData = {};
        var _this = $(this).parents('form.contact-form');
        var contact_form_id = _this.find('input[name=contact-form-id]').val();
        var form_url = _this.attr("action");

		if(_this.parents('.jetpack-forms-wrapper').parent('div').attr('data-form_type')){
			formType = _this.parents('.jetpack-forms-wrapper').parent('div').attr('data-form_type');
		}else if(_this.parents('.jetpack-forms-inner').attr('data-form_type')){
			formType = _this.parents('.jetpack-forms-inner').attr('data-form_type');
		}
		 
        if(_this.parent('#contact-form-'+contact_form_id).parent('div').hasClass('footer-form')){
            formType = 'Footer Form';
        }
        var emailaddress = _this.find('input[name=g' + contact_form_id + '-emailaddress]');
        var err_wrapper = _this.find('.jetpack-form-ajax-status-wrapper');
        var success_msg = "";

		if(_this.parents('.jetpack-forms-wrapper').parent('div').attr('data-form_thanks_msg')){
			success_msg = _this.parents('.jetpack-forms-wrapper').parent('div').attr('data-form_thanks_msg');
		}else if(_this.parents('.jetpack-forms-inner').attr('data-form_thanks_msg')){
			success_msg = _this.parents('.jetpack-forms-inner').attr('data-form_thanks_msg');
		}

        //Get our form fields here. New fields can be added here as well.
        if(formType != 'Footer Form'){
            var firstname = _this.find('input[name=g' + contact_form_id + '-firstname]');
            var lastname = _this.find('input[name=g' + contact_form_id + '-lastname]');
            var jobtitle = _this.find('input[name=g' + contact_form_id + '-jobtitle]');
            var companytype = _this.find('select[name=g' + contact_form_id + '-companytype]');
            var joblevel = _this.find('select[name=g' + contact_form_id + '-joblevel]');
            var jobrole = _this.find('select[name=g' + contact_form_id + '-jobrole]');
            var annualspend = _this.find('select[name=g' + contact_form_id + '-annualspend]');
            var companyname = _this.find('input[name=g' + contact_form_id + '-companyname]');
            var country = _this.find('select[name=g' + contact_form_id + '-country]');
            var jpregion = _this.find('select[name=g' + contact_form_id + '-region]');
            var message = _this.find('textarea[name=g' + contact_form_id + '-message]');
            var usstates, city = '';

			if(_this.find('.company-type-select').length > 0){
				var companyTypeSpend = (_this.find('.company-type-select').attr('data-company-spend')).split('#');
			}

			var jobLevelName = _this.find('.job-level-select').attr('data-optionName');
			var companyTypeName = _this.find('.company-type-select').attr('data-optionName');
			var jobRoleName = _this.find('.job-role-select').attr('data-optionName');
			var annualSpendName = _this.find('.annual-spend-select').attr('data-optionName');

			var USA = _this.find('.select-country').attr('data-us');
			var GER = _this.find('.select-country').attr('data-gr');

            if(country.val() == USA){
                usstates = _this.find('select[name=g' + contact_form_id + '-usstates]');

                if(usstates.val() == "CA"){
                    city = _this.find('select[name=g' + contact_form_id + '-californiacity]');
                }else if(usstates.val() == "FL"){
                    city = _this.find('select[name=g' + contact_form_id + '-floridacity]');
                }else if(usstates.val() == "TX"){
                    city = _this.find('select[name=g' + contact_form_id + '-texascity]');
                }
            }else if(country.val() == GER){
                city = _this.find('select[name=g' + contact_form_id + '-germancity]');
            }

            if(firstname.val() == ''){
                firstname.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+firstname.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(lastname.val() == ''){
                lastname.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+lastname.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(emailaddress.val() == ''){
                emailaddress.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+emailaddress.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(!isValidEmailAddress(emailaddress.val())){
                emailaddress.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+emailaddress.parents('.field-err').attr('data-invalid')+'</p>');

                setCookie('user_tried_with_error', 'yes', 30);
                return false;
            }else if(jobtitle.val() == ''){
                jobtitle.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+jobtitle.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(joblevel.length > 0 && (joblevel.val() == jobLevelName || joblevel.val() === null)){
                joblevel.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+joblevel.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(companytype.length > 0 && (companytype.val() == companyTypeName || companytype.val() === null)){
                companytype.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+companytype.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(jobrole.length > 0 && (jobrole.val() == jobRoleName || jobrole.val() === null)){
                jobrole.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+jobrole.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(companyname.val() == ''){
                companyname.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+companyname.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(country.length > 0 && (country.val() == USA && (usstates.val() == 'State' || usstates.val() === null))){
                usstates.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+usstates.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(country.length > 0 && (((country.val() == USA && (usstates.val() == "CA" || usstates.val() == "FL" || usstates.val() == "TX") && (city.val() == 'Nearest City' || city.val() === null))) || (country.val() == GER && (city.val() == 'Nearest City' || city.val() === null)))){
                city.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+city.parents('.field-err').attr('data-err')+'</p>');
                return false;
            }else if(formType == 'Download GIR Japan' && jpregion.val() == '地域'){
                jpregion.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+jpregion.parents('.field-err').attr('data-err')+'</p>');
                return false;
            }else if((jQuery.inArray(companytype.val(), companyTypeSpend) >= 0) && annualspend.val() == annualSpendName || annualspend.val() === null){
                annualspend.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+annualspend.parents('.field-err').attr('data-err')+'</p>');

                return false;
            }else if(formType == 'Contact Form' && message.val() == ''){
                message.parents('.text-area-label, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">'+message.parents('.field-err').attr('data-err')+'</p>');
                return false;
            }else if(_this.find('.grunion-field-checkbox-wrap input.checkbox').prop('checked') == false){
                err_wrapper.prepend('<p class="error-alert">'+_this.parents('.jetpack-forms-inner').attr('data-consent-err')+'</p>');
                return false;
            }
            else{
            	formData['emailaddress'] = emailaddress.val();
            	formData['firstname'] = firstname.val();
            	formData['lastname'] = lastname.val();
            	formData['jobtitle'] = jobtitle.val();
            	formData['companytype'] = companytype.val();
            	formData['companyname'] = companyname.val();
            	formData['country'] = country.val();
            	formData['message'] = message.val();
            	if(country.val() == USA){
	            	formData['usstates'] = usstates.val();
	            }
            	if(city.length > 0 && (country.val() == USA && (usstates.val() == "CA" || usstates.val() == "FL" || usstates.val() == "TX") || country.val() == GER)){
	            	formData['city'] = city.val();
	            }
                $('.placeholder-wrapper, .grunion-field-wrap').removeClass('error-field');
                $('.jetpack-form-ajax-status-wrapper').html('');
            }
        }else{
	
            if(emailaddress.val() == ''){
                emailaddress.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">Please fill out the required fields.</p>');

                return false;
            }else if(!isValidEmailAddress(emailaddress.val())){
                emailaddress.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
                err_wrapper.prepend('<p class="error-alert">Please use your business email address.</p>');

                setCookie('user_tried_with_error', 'yes', 30);
                return false;
            }else{
            	formData['emailaddress'] = emailaddress.val();
            }
        }

        // get/set other form data
        //If the user is logged in, we need these values as well for the form to submit correctly.
        //WorkSpace is an object that stores values from PHP. In this case, WorkSpace. User is boolean (true/false) and is set using is_user_logged_in() WP Function
        //////if (WorkSpace.User) {
            formData['_wpnonce']         = _this.find('input[name=_wpnonce]').val();
            formData['_wp_http_referer'] = _this.find('input[name=_wp_http_referer]').val();
        //////}
        //These fields are required in all cases
        formData['contact-form-id']  = contact_form_id;
        formData['action']           = 'grunion-contact-form';

        //Loading animation. Uses Font Awesome but could be replaced with whatever.
        _this.find('input[type=submit]').after('<span class="spinner"><i class="fa fa-spinner fa-spin fa-3x fa-fw" aria-hidden="true"></i></span>');

        //--------------- ajax removed -----
        var form = '#contact-form-' + contact_form_id;

        //Hide the spinner animation
        $(form).find('.spinner').hide();


        if(getCookie('_mkto_trk') != ""){
	    	mkt_cookie = getCookie('_mkto_trk');
	    }

        //Footer form code
        if(formType == 'Footer Form'){
        var footer_payload = {email: payload['email']};
        mkt_formID = 1120;
        success_msg = "<p>Thanks for signing up for our newsletter!<br>We will be in touch with you shortly.</p>";

          $.ajax({
            url: window.location.origin + "/wp-admin/admin-ajax.php",
            data:'payload=' + JSON.stringify(footer_payload) + '&formId=' + mkt_formID + '&action=submit_marketo_form', 
            type:'POST',
            dataType: 'json',
            dataFilter: function(data, type) {
                if(type == 'json' ) {
                	return JSON.stringify({formType: formType});
                }
            },
            success:function(data, success, xhr){
              	//console.log(data);
			  	var response = jQuery.parseJSON(xhr.responseText);
				//console.log(response);
				if(response.success == true){
					footer_payload.webFormType = 'Footer Form';
					$.ajax({
						url: "https://xenopsigrowthpartners.com/dv_form_hook.php",
						data:'payload=' + JSON.stringify(footer_payload) + '&formId=' + mkt_formID + '&response='+JSON.stringify(response),
						type:'POST',
						dataType: 'json',
						//id:submittedID.requestId,status:submittedID.result[0].status
						success:function(data){

						}
					});

					_this.addClass('form-invisible');
					$('.getInTouchThanks').html(success_msg);
				}else{
					emailaddress.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
					err_wrapper.prepend('<p class="error-alert">Please fill out the required fields.</p>');
	
					//ga('send', 'event', 'Mandatory fields missing', 'Submit' );
					return false;
				}
            }
          });
        }else{
        	if(formType == "Webinar Event" || formType == "In Person Event"){
        		if(jp_contact_form){
        			mkt_formID = 1447;
        		}else{
	        		if($('body').hasClass('postid-727206')){
	        			delete payload.Message__c;
	        			delete payload.city;
	    			}
					mkt_formID = 1166;
	        	}
        	}else if(formType == "Event"){
				delete payload.Message__c;
				delete payload.city;
				delete payload.state;
				delete payload.country;
				delete payload.jobLevel;
				delete payload.jobRole;
				delete payload.annualMediaSpend;
				delete payload.formSource;
	        	mkt_formID = 1167;
			}else if(formType == "Publicis Newsletter" && $('body').hasClass('page-id-11032')){
				delete payload.consentDate;
				delete payload.formSource;
				delete payload.webFormType;
				delete payload.city;
				delete payload.state;
				delete payload.country;
				delete payload.jobLevel;
				delete payload.jobRole;
				delete payload.annualMediaSpend;
				mkt_formID = 1381;
			}else if(formType == "DPA Signup"){
				delete payload.Message__c;
				delete payload.formSource;
				delete payload.consentDate;
				delete payload.webFormType;
				delete payload.city;
				delete payload.state;
				delete payload.country;
				delete payload.jobLevel;
				delete payload.jobRole;
				delete payload.annualMediaSpend;
				delete payload.consent;
				mkt_formID = 1683;
			}else{
				if(formType != "Publicis Newsletter" && formType != 'Footer Form' && formType != 'Footer Form'){
					mkt_formID = 1046;
				}

			}

			if(mkt_formID == 1046){
				//payload['gIRAPAC'] = payload['gIREMEA'] = payload['gIRGlobal'] = payload['gIRLATAM'] = payload['gIRNA'] = false;
				if(form_custom_value == 'APAC'){
					//payload['gIRAPAC'] = true;
					payload['gIRregion'] = 'APAC';
				}else if(form_custom_value == 'EMEA'){
					//payload['gIREMEA'] = true;
					payload['gIRregion'] = 'EMEA';
				}else if(form_custom_value == 'Global'){
					//payload['gIRGlobal'] = true;
					payload['gIRregion'] = 'GLOBAL';
				}else if(form_custom_value == 'LATAM'){
					//payload['gIRLATAM'] = true;
					payload['gIRregion'] = 'LATAM';
				}else if(form_custom_value == 'NA'){
					//payload['gIRNA'] = true;
					payload['gIRregion'] = 'NA';
				}else if(form_custom_value == 'English'){
					payload['downloadEnglish'] = true;
				}else if(form_custom_value == 'Indonesian'){
					payload['downloadIndonesian'] = true;
				}else if(form_custom_value == 'Vietnamese'){
					payload['downloadVietnamese'] = true;
				}else if(form_custom_value == 'Thai'){
					payload['downloadThai'] = true;
				}
			}

			$.ajax({
				url: window.location.origin + "/wp-admin/admin-ajax.php",
				data:'payload=' + encodeURIComponent(JSON.stringify(payload)) + '&formId=' + mkt_formID + '&action=submit_marketo_form',
				type:'POST',
				dataType: 'json',
				dataFilter: function(data, type) {
				    if(type == 'json' ) {
				    	if(formType == "Contact Form" && ($('body').hasClass('page-id-11505') || $('body').hasClass('page-id-11506') || $('body').hasClass('page-id-11507'))){
				    		return JSON.stringify({formType: "SEM Pages"});
				    	}else if(formType == "Download PDF" && ($('body').hasClass('page-id-10009') || $('body').hasClass('page-id-573156') || $('body').hasClass('page-id-1112449'))){
				    		return JSON.stringify({formType: "Goal Download"});
				    	}else{
						if(form_custom_value != ''){
							return JSON.stringify({region: form_custom_value, formType: formType});
						}else{
							return JSON.stringify({formType: formType});
						}
				    	}
			        }
				},
				success:function(data, success, xhr){
					var submittedID = jQuery.parseJSON(xhr.responseText);
					//console.log(submittedID.success);
					if(submittedID.success == true){
						// $.ajax({
						// 	url: "https://xenopsigrowthpartners.com/dv_form_hook.php",
						// 	data:'payload=' + encodeURIComponent(JSON.stringify(payload)) + '&formId=' + mkt_formID + '&response='+JSON.stringify(submittedID),
						// 	type:'POST',
						// 	dataType: 'json',
						// 	//id:submittedID.requestId,status:submittedID.result[0].status
						// 	success:function(data){

						// 	}
						// });
						submittedID = submittedID.requestId;
						submittedID = encodeURIComponent(window.btoa(submittedID));

						_this.parents('.jetpack-forms-inner').addClass('form-invisible');


						if(formType != "In Person Event" || formType != "Event" || formType != "Webinar Event"){
							var blog_cat = blog_type = "";
							if(getCookie('wp_visited_post_cat') != ""){
								blog_cat = getCookie('wp_visited_post_cat');
							}
							if(getCookie('wp_visited_post_tag') != ""){
								blog_type = getCookie('wp_visited_post_tag');
							}
							
						}

						if(getCookie('user_tried_with_error') == 'yes'){

							deleteCookie('user_tried_with_error');
						}

						if(DV_Custom_Contextual == true){

							_linkedin_partner_id = "2362060";
							window._linkedin_data_partner_ids = window._linkedin_data_partner_ids || [];
							window._linkedin_data_partner_ids.push(_linkedin_partner_id);
							
							(function(){var s = document.getElementsByTagName("script")[0];
							var b = document.createElement("script");
							b.type = "text/javascript";b.async = true;
							b.src = "https://snap.licdn.com/li.lms-analytics/insight.min.js";
							s.parentNode.insertBefore(b, s);})();
							
							$("body").append('<img height="1" width="1" style="display:none;" alt="" src="https://px.ads.linkedin.com/collect/?pid=2362060&fmt=gif" />');	
						}

						if(formType == "Download PDF"){
							_this.parents('.jetpack-forms-wrapper').append("<div class='getInTouchThanks'><p>"+success_msg+"</p></div>");

							$("body").append('<img height="1" width="1" style="display:none;" alt="" src="https://px.ads.linkedin.com/collect/?pid=2362060&conversionId=2578066&fmt=gif" /><img src="https://secure.adnxs.com/px?id=1302922&t=2" width="1" height="1" />');
				
							var script = document.createElement("script"); //Make a script DOM node
							script.src = '//pixel.mathtag.com/event/js?mt_id=1439943&mt_adid=229936&mt_exem=&mt_excl=&v1=&v2=&v3=&s1=&s2=&s3='; //Set it's src to the provided URL
							document.head.appendChild(script); //Add it
							
							var script = document.createElement("script"); //Make a script DOM node
							script.src = 'https://secure.adnxs.com/px?id=1302922&t=1'; //Set it's src to the provided URL
							document.head.appendChild(script); //Add it

							//fbq('track', 'Lead', {content_name: 'CTV download'});

							
							if(donwloadFile){
								if(isBlockedEmailAddress(emailaddress.val())){
									setTimeout(function(){ 
										//window.location.replace(donwloadFile);
										window.open(donwloadFile, '_blank');
									}, 500);
								}
							}
							
						}else if(formType == "Contact Form"){
							_this.parents('.jetpack-forms-wrapper').append("<div class='getInTouchThanks'><p>"+success_msg+"</p></div>");

							$("body").append('<img height="1" width="1" style="display:none;" alt="" src="https://px.ads.linkedin.com/collect/?pid=2362060&conversionId=4500116&fmt=gif" />');
							
							var script = document.createElement("script"); //Make a script DOM node
							
							script.src = '//pixel.mathtag.com/event/js?mt_id=1479155&mt_adid=229936&mt_exem=&mt_excl=&v1=&v2=&v3=&s1=&s2=&s3='; //Set it's src to the provided URL

							document.head.appendChild(script); //Add it to the end of the head section of the page (could change 'head' to 'body' to add it to the end of the body section instead)
							
							//fbq('track', 'Lead', {content_name: 'Get In Touch'});
							//gtag_report_conversion();
						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-8332465')) || (formType == "Webinar Event" && $('body').hasClass('page-id-8347107'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/context-matters-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-9889778'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/ctv-attention-webinar-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-9892971'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/2024-global-insights-apac-webinar-recording-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-9892926'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/2024-global-insights-emea-webinar-recording-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-9892957'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/2024-global-insights-americas-webinar-recording-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-10111331'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/2024-global-insights-japan-webinar-recording-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-10498848'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/back-to-basics-how-to-measure-and-maximize-consumer-attention-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-10115925'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/back-to-basics-demystifying-made-for-advertising-sites-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-10158136'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/back-to-basics-demystifying-made-for-advertising-sites-video-jp/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-10156678'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/back-to-basics-brand-safety-and-suitability-insights-to-navigate-election-season-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-10857871'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/the-state-of-streaming-todays-trends-shaping-tvs-future-recording-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-10957894'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/2025-dv-global-insights-apac-webinar-recording-video/?dvfsc='+submittedID;

						}else if((formType == "Webinar Event" && $('body').hasClass('page-id-10985607'))){
							_this.parents('.jetpack-forms-wrapper').append('');
							setCookie('form_submission_id', submittedID, 5);
							window.location = '/2025-dv-global-insights-emea-webinar-recording-video/?dvfsc='+submittedID;

						}else{
							if(event_form_success_msg != "" && typeof event_form_success_msg != "undefined"){
								_this.parents('.jetpack-forms-wrapper').append('<div class="getInTouchThanks">'+event_form_success_msg+'</div>');
							}else{
								_this.parents('.jetpack-forms-wrapper').append("<div class='getInTouchThanks'><p>"+success_msg+"</p></div>");
							}
						}
					}else{
						emailaddress.parents('.placeholder-wrapper, .grunion-field-wrap').addClass('error-field');
						err_wrapper.prepend('<p class="error-alert">Please fill out the required fields.</p>');
		
						//ga('send', 'event', 'Mandatory fields missing', 'Submit' );
						return false;
					}
				}
			});
        }

    });
	//NEW CODE ENDS

})(jQuery);

function submit_mkt_frm(payload,mkt_formID){
	var mkt_cookie = pageURL = '';
	//data:'payload=' + JSON.stringify(payload) + '&formId=' + mkt_formID + '&cookie=' + mkt_cookie + '&pageURL=' + pageURL + '&action=submit_marketo_form', // form data
	$.ajax({
		url: window.location.origin + "/wp-admin/admin-ajax.php",
		data:'payload=' + JSON.stringify(payload) + '&formId=' + mkt_formID + '&action=submit_marketo_form', // form data
		type:'POST', // POST
		success:function(data){
			console.log(data);
		}
	});
}
;
;
$(document).ready(function(){
	 // Post per page
	var ppp = 12;
	var pageNumber = {
		val:1,
	};
	var ajaxLoad = {
		success:true
	};

	$('[name=category], [name=post_tag]').on('click', function(){
		pageNumber.val = 1;
	});

	$(document).on("click",'.blog-items-categories .btn',function(){ // When btn is pressed.
		var ppp = -1;
		var tagID = $(this).attr('data-tag');
		var $btn = $(this);
		var catId = $(this).attr('data-category');
		//var pageNumber = parseInt($btn.attr('data-page'));
		var $loader = $btn.parent().siblings('.blog-items-wrap');
		//pageNumber++;
		//var loadedPost = $btn.attr('data-loaded');
		//$btn.attr('data-page', pageNumber);
		load_posts(tagID, $btn, $loader, catId, ppp);
		
	});

	$(document).on("click",'.spotlight-loadmore-cta',function(){ // When btn is pressed.
		var ppp = -1;
		var tagID = $(this).attr('data-tag');
		var $btn = $(this);
		var catId = $(this).attr('data-category');
		//var pageNumber = parseInt($btn.attr('data-page'));
		var $loader = $('.latest-post-wrap');
		var page_id = $('[name=page_id]').val();
		var jp_page = '';
		jp_page = $(this).attr('data-jp-page');
		//pageNumber++;
		//var loadedPost = $btn.attr('data-loaded');
		//$btn.attr('data-page', pageNumber);
		load_posts3(tagID, $btn, $loader, catId, ppp, page_id, jp_page);
		
	});

	var lastScrollTop = 0;
	$(window).scroll(function() {

		var scrollT = $(this).scrollTop();
		if (scrollT > lastScrollTop){
			if($('.blog-item-container').hasClass('tag-blog')){
				//console.log($(window).scrollTop() + $(window).height());
				//console.log($(document).height() - 100);
				//if ($(window).scrollTop() + $(window).height()  >= $(document).height() - 100) {
	
				if ($(window).scrollTop() >= $('.static-blog-posts-list').offset().top + $('.static-blog-posts-list').
					outerHeight() + 80 - window.innerHeight) {
					disableScroll();
					var $btn = $('.loadmore-cta');
					var tagID = $btn.attr('data-tag');
					var catId = $btn.attr('data-category');
					//var pageNumber = parseInt($btn.attr('data-page'));
					var $loader = $('.blog-item-container.tag-blog .blog-items-wrap');
					if(ajaxLoad.success === true){
						load_posts2(tagID, $btn, $loader, catId, ppp);
					}
					return false;
				}
			}
		} else {
			enableScroll();
		}
		lastScrollTop = scrollT;

		
	});
	
	//+ '&pageNumber=' + pageNumber
	function load_posts(tagID, $btn, $loader, catId, ppp){
		//pageNumber++;
		$('.blog-items-wrap').addClass('loading');
		var str = '&tag_id=' + tagID +'&cat=' + catId + '&ppp=' + ppp + '&action=more_post_ajax';
		$.ajax({
			type: "POST",
			dataType: "html",
			url: ajax_posts.ajaxurl,
			data: str,
			async: true,
			success: function(data){
				$('.blog-items-wrap').removeClass('loading');
				var $data = $(data);
				if($data.length){
					$('.blog-item-container .blog-post').show();
					$loader.html($data);
					$btn.remove();
					//$("#more_posts").attr("disabled",false);
				} else{
					//$btn.remove();
					
				}
			},
			error : function(jqXHR, textStatus, errorThrown) {
				$('.blog-items-wrap').removeClass('loading');
				$loader.html(jqXHR + " :: " + textStatus + " :: " + errorThrown);
			}

		});
	}

	function load_posts3(tagID, $btn, $loader, catId, ppp, page_id,jp_page){
		//pageNumber++;
		$('.latest-post-wrap').addClass('loading');
		var str = '&tag_id=' + tagID +'&cat=' + catId + '&ppp=' + ppp + '&action=more_post_ajax&page_name=spotlight&page_id='+page_id+'&jp_page='+jp_page;
		$.ajax({
			type: "POST",
			dataType: "html",
			url: ajax_posts.ajaxurl,
			data: str,
			async: true,
			success: function(data){
				$('.latest-post-wrap').removeClass('loading');
				var $data = $(data);
				if($data.length){
					$('.blog-item-container .blog-post').show();
					$loader.html($data);
					$btn.hide();
					//$("#more_posts").attr("disabled",false);
				} else{
					//$btn.remove();
					
				}
			},
			error : function(jqXHR, textStatus, errorThrown) {
				$('.latest-post-wrap').removeClass('loading');
				$loader.html(jqXHR + " :: " + textStatus + " :: " + errorThrown);
			}

		});
	}

	function load_posts2(tagID, $btn, $loader, catId){
		pageNumber.val += 1;
		ajaxLoad.success = false;
		$('.blog-items-wrap').addClass('loading');
		var str = '&pageNumber=' + pageNumber.val + '&tag_id=' + tagID +'&cat=' + catId + '&ppp=' + ppp + '&pageType=blog&action=more_post_ajax';
		$.ajax({
			type: "POST",
			dataType: "html",
			url: ajax_posts.ajaxurl,
			data: str,
			async: true,
			success: function(data){
				$('.blog-items-wrap').removeClass('loading');
				var $data = $(data);
				if($data.length){
					//$loader.append('<h2>'+pageNumber.val+'</h2>');
					$('.blog-item-container .blog-post').show();
					$loader.append($data);
					//$("#more_posts").attr("disabled",false);
				} else{
					//$btn.remove();
					
				}
				enableScroll();
				ajaxLoad.success = true;
			},
			error : function(jqXHR, textStatus, errorThrown) {
				$('.blog-items-wrap').removeClass('loading');
				$loader.html(jqXHR + " :: " + textStatus + " :: " + errorThrown);
			}

		});
		return false;
	}

	function disableScroll() {
	    // Get the current page scroll position
	    scrollTop = window.pageYOffset || document.documentElement.scrollTop;
	    scrollLeft = window.pageXOffset || document.documentElement.scrollLeft,
	  
	        // if any scroll is attempted, set this to the previous value
	        window.onscroll = function() {
	            window.scrollTo(scrollLeft, scrollTop);
	        };
	}
	  
	function enableScroll() {
	    window.onscroll = function() {};
	}
	
});
;
